{
  "manifest_version": 2,
  "name": "Hüma Gezgini",
  "version": "1",
  "description": "Hüma Web Tarayıcısı için Hüma Gezgini",
  "icons": {
    "48": "Images/humaGezginIcon48.png",
    "128": "Images/humaGezginIcon128.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "Images/humaGezginIcon16.png",
      "32": "Images/humaGezginIcon32.png"
    },

    "theme_icons": [
      {
        "light": "Images/humaGezginIcon16-dark.png",
        "dark": "Images/humaGezginIcon16.png",
        "size": 16
      },
      {
        "light": "Images/humaGezginIcon32-dark.png",
        "dark": "Images/humaGezginIcon32.png",
        "size": 32
      }
    ],

    "default_title": "humaGezgin",
    "default_popup": "Popup/popup.html"
  },

  "background": {
    "scripts": ["Popup/popup.js"]
  },

  "permissions": [
    "activeTab", "tabs"
  ]
}
